{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bignumber.js": "^7.2.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4",
    "truffle-contract": "^4.0.0-next.0",
    "web3": "^1.0.0-beta.35"
  },
  "scripts": {
    "start-css": "yarn run build-css --watch",
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "run-p start-css start-js",
    "build": "run-s build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "link-contracts": "run-script-os",
    "link-contracts:linux:darwin": "cd src && ln -s ../../build/contracts contracts",
    "link-contracts:win32": "cd src && mklink \\D contracts ..\\..\\build\\contracts"
  },
  "devDependencies": {
    "bulma": "^0.7.1",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "run-script-os": "^1.0.3"
  }
}
